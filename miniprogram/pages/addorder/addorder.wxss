page{
  background-color: #eee;
}

.textone{
  font-size: 35rpx;
  width: 100%;
}

.container {
  background-color: #F2f2f2;
  min-height: 100%;
  display: block;
}

.form-container{
  width: 100%;
  margin-top: 20rpx;
  
}

.row{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  margin-bottom: 5rpx;
  display: flex;
  font-size: 28rpx;
  
}

.row .rowleft{
  width: 160rpx;
  color: #000;
  background-color: #fff;
  text-align: center;
} 

.row .rowright{
  flex: 1;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #fff;
} 

.row .rowright .input{
  height: 100%;
  font-size: 28rpx;
  padding-right: 30rpx;
} 

.button{
  width: 60%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 60rpx;
  background-color: #267fd9;
  border-radius: 10px;
  position:fixed;
  left:0;
  right:0;
  bottom:0;
  color: white;
}

.text{
  width: 100%;
  height: 400rpx;
  font-size: 26rpx;
  text-align: center;
  margin-top: 100rpx;
  color: gray;
}

.picker{
  margin-top: 10rpx;
}
